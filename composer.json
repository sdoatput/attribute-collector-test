{
    "name": "put/attribute-collector-test",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "packages/attribute",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "~8.2.0",
        "put/attribute": "*",
        "olvlvl/composer-attribute-collector": "v1.2.2"
    },
    "autoload": {
        "psr-4": {
            "PUT\\AttributeCollectorTest\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "olvlvl/composer-attribute-collector": true
        }
    }
}
